<%= simple_form_for(@cocktail) do |f| %>
  <%= f.input :name %>
  <%= f.input :ingredient %>
  <%= f.submit "add cocktail" class='btn btn-primary' %>
<% end %>

<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :category, type: 'select', collection: %w[chinese italian japanese french belgian] %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.submit %>
<% end %>
